  "bugs": {
    "url": "https://github.com/5d-jh/school-menu-api/issues"
  },
  "homepage": "https://github.com/5d-jh/school-menu-api#readme",
  "dependencies": {
    "@google-cloud/firestore": "^3.3.4",
    "entities": "^2.0.0",
    "jquery": "^3.4.1",
    "jsdom": "^12.2.0",
    "nunjucks": "^3.2.0",
    "path-to-regexp": "^6.1.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^1.3.2",
    "mocha": "^7.0.0"
  }
